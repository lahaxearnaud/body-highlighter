{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ES2019",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["dom", "es2019"],
    "baseUrl": ".",
    "types": ["node"],
    "strict": true
  }
}
